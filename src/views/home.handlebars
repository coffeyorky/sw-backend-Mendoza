 <h1>Lista de productos</h1>
 <ul class="dirigir">
      {{#each productos}}
<div class="caja">
      <img alt={{this.title}} src={{this.imagen}} width="200"/>
      <h2>{{this.title}}</h2>
      <h4>{{this.category}}</h4>
      <p>{{this.description}}</p>
      <h3>Stock: {{this.stock}}</h3>
      <h3>${{this.price}}</h3>
       <button class="btn">Agregar al carrito</button>
     </div>
    {{/each}}  
 </ul>
 

 

