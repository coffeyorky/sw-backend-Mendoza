<ul>
  {{#each products}}
  <div class="caja">
    <h2>{{this.title}}</h2>
    <h4>{{this.category}}</h4>
    <p>{{this.description}}</p>
    <h3>{{this.price}}</h3>
    <button>Agregar al carrito</button>
</div>
  {{/each}}
</ul>

<div>
    {{#if hasPrevPage}}
    <a href="/api/productos?page={{prevPage}}"><button>Anterior</button></a>
  {{/if}}
  {{#if hasNextPage}}
    <a href="/api/productos?page={{nextPage}}"><button
        type="button"
        class="btn btn-primary"
      >Siguiente</button></a>
  {{/if}}
</div>